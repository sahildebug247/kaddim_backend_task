		"@types/passport-jwt": "^3.0.6",
		"@types/supertest": "^2.0.9",
		"@types/uuid": "^8.0.0",
		"concurrently": "^5.2.0",
		"jest": "^26.0.1",
		"nodemon": "^2.0.4",
		"prettier": "^2.0.5",
		"supertest": "^4.0.2",
		"ts-jest": "^26.0.0",
		"ts-node": "^8.10.1",
		"tsconfig-paths": "^3.9.0",
		"tslint": "^6.1.2",
		"typescript": "^3.8.3",
		"wait-on": "^5.0.0"
	},
	"dependencies": {
		"@hapi/joi": "^17.1.1",
		"@nestjs/common": "^7.0.13",
		"@nestjs/core": "^7.0.13",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/mongoose": "^8.0.0",
